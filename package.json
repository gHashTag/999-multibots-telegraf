{
  "name": "neuro-blogger-telegram-bot",
  "version": "0.0.1",
  "description": "Neuro Blogger Telegram Bot",
  "author": "",
  "license": "ISC",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "predev": "node scripts/kill-port.js 2999 3001",
    "dev": "cross-env NODE_ENV=development nodemon -r tsconfig-paths/register",
    "dev:single": "cross-env NODE_ENV=development BOT_NAME=$BOT_NAME ts-node -r tsconfig-paths/register src/bot.ts",
    "start": "npm run build && cross-env NODE_ENV=production node -r tsconfig-paths/register dist/server.js",
    "bots": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/bot.ts",
    "build": "swc src -d dist --source-maps --copy-files && tsc-alias -p tsconfig.paths.json && tsc --emitDeclarationOnly -p tsconfig.json",
    "build:tsc": "tsc -p tsconfig.json && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "test:memory": "NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/zepMemory.test.ts",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "ngrok": "ngrok http 2999",
    "tunnel": "ssh -R 80:localhost:2999 localhost.run",
    "dev:inngest": "npx inngest-cli@latest dev -u http://localhost:2999/api/inngest",
    "type-check": "ts-node -r tsconfig-paths/register ./node_modules/.bin/tsc --noEmit -p tsconfig.json",
    "check-staged-types": "ts-node scripts/check-staged-types.ts",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "deploy": "./deploy.sh",
    "tsc": "tsc --noEmit -p tsconfig.json"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "npm run check-staged-types"
    ]
  },
  "dependencies": {
    "@inngest/test": "^0.1.6",
    "@mtproto/core": "^6.3.0",
    "@supabase/supabase-js": "^2.49.4",
    "@web-std/file": "^3.0.3",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "elevenlabs": "^1.56.1",
    "envalid": "^7.3.1",
    "esbuild": "^0.25.2",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "form-data": "^4.0.1",
    "inngest": "^3.32.7",
    "jest-mock": "^29.7.0",
    "jszip": "^3.10.1",
    "md5": "^2.3.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "^2.7.0",
    "openai": "^4.77.0",
    "pinata-web3": "^0.5.4",
    "replicate": "^0.32.0",
    "telegraf": "^4.16.3",
    "tslib": "^2.8.1",
    "undici": "^7.7.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/md5": "^2.3.5",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.19.86",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^13.3.0",
    "node-config": "^0.0.2",
    "node-gyp": "^9.1.0",
    "nodemon": "^2.0.19",
    "pm2": "^5.2.0",
    "prettier": "^3.0.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  }
}
