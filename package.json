{
  "name": "neuro-blogger-telegram-bot",
  "version": "1.0.0",
  "description": "Neuro Blogger Telegram Bot",
  "author": "",
  "license": "ISC",
  "scripts": {
    "predev": "node scripts/kill-port.js 2999 3001 8288",
    "dev": "concurrently \"npm run bots\" \"npm run dev:inngest\"",
    "deploy": "ssh -i ~/.ssh/id_rsa root@999-multibots-u14194.vm.elestio.app 'cd /opt/app/999-multibots-telegraf && git pull && chmod +x *.sh && ./update-docker.sh'",
    "inngest:dev": "inngest-cli dev -u http://localhost:3000/api/inngest --no-discovery --no-watch --no-telemetry",
    "dev:inngest": "npx inngest-cli@latest dev -u http://localhost:2999/api/inngest --port 8288",
    "dev:single": "cross-env NODE_ENV=development BOT_NAME=$BOT_NAME ts-node -r tsconfig-paths/register src/bot.ts",
    "start": "npm run build && cross-env NODE_ENV=production node dist/bot.js",
    "bots": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/bot.ts",
    "build": "pnpm build:js && pnpm build:types",
    "build:js": "esbuild --bundle --platform=node --target=node16 --outdir=dist/ --packages=external --format=cjs src/index.ts src/start-agent.ts src/run-test.ts",
    "build:types": "tsc",
    "dev:agent": "nodemon src/start-agent.ts -e ts",
    "devs": "pnpm dev:agent & pnpm dev:inngest",
    "lint": "eslint --ext .ts . --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "services": "tsx src/api-server.ts",
    "check-port": "node scripts/check-webhook.js",
    "dev:services": "tsx src/api-server.ts",
    "dev:bots": "cd src && tsx watch bots.ts",
    "dev:api": "cd src && tsx watch api.ts",
    "dev:debug-api": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/debug-api.ts",
    "debug-api": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/debug-api.ts",
    "inngest-connect": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/inngest-connect.ts",
    "inngest-standalone": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/inngest-standalone.ts",
    "prettier": "prettier --write .",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "generateTypes": "supabase gen types typescript --linked > src/types/supabase.ts",
    "release": "release-it",
    "telegrambot:dev": "rm -rf ./.next && tsc-watch --onSuccess \"node --experimental-specifier-resolution=node dist/bot/index.js\"",
    "telegrambot": "npx tsc && node dist/bot/index.js",
    "telegrambot:debug": "npx tsc && node --inspect dist/bot/index.js",
    "tgwebhook:dev": "NODE_ENV=development tsc-watch --onSuccess \"node --experimental-specifier-resolution=node dist/webhook/index.js\"",
    "tgwebhook": "NODE_ENV=production node dist/webhook/index.js",
    "ngrok": "ngrok http 2999",
    "tunnel": "ssh -R 80:localhost:2999 localhost.run",
    "type-check": "tsc --noEmit",
    "prepare": "husky install || true",
    "prebuild": "rm -rf dist",
    "prepare:assets": "node scripts/prepare-assets.js",
    "test": "ts-node src/run-test.ts",
    "test:api": "cross-env NODE_ENV=test npx tsx src/test-utils/scripts/test-api.ts",
    "test:database": "ts-node src/run-test.ts database",
    "test:inngest": "npx tsx src/test-utils/tests/runInngestTests.ts",
    "test:neuro": "ts-node src/run-test.ts neuro",
    "test:payment": "ts-node src/run-test.ts payment",
    "test:speech": "ts-node src/run-test.ts speech",
    "test:translations": "ts-node src/run-test.ts translations",
    "test:webhook": "npx tsx src/test-utils/index.ts --category=webhook",
    "test:system": "npx tsx src/test-utils/index.ts --category=system",
    "test:mock": "npx tsx src/test-utils/index.ts --test=mockFnTest",
    "test:agent-router": "npx tsx src/test-utils/index.ts --category=agent-router",
    "test:agents": "npx tsx src/test-utils/index.ts --category=agents",
    "test:agents-only": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/scripts/run-agent-tests.ts",
    "test:code-analysis-agent": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/test-utils/tests/agent/runAgentTests.ts",
    "test:payment-processor": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/payment/paymentProcessorTest.ts",
    "test:payment-create": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/payment/createSuccessfulPaymentTest.ts",
    "test:payment-notification": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/payment/paymentNotification.test.ts",
    "test:ru-payment": "npx tsx src/test-utils/index.ts --category=ru-payment",
    "test:neurophoto": "ts-node src/test-utils/direct-neuro-photo-test.ts",
    "test:neurophoto:gq": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/neuro/gq-portrait/testNeuroPhotoGQ.ts",
    "test:neurophoto:gq-batch": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/neuro/gq-portrait/testNeuroPhotoGQBatch.ts",
    "test:neurophoto:gq-business": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/neuro/gq-portrait/testNeuroPhotoGQBusiness.ts",
    "test:neurophoto:gq-fashion": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/neuro/gq-portrait/testNeuroPhotoGQFashion.ts",
    "test:neurophoto:gq-artistic": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/neuro/gq-portrait/testNeuroPhotoGQArtistic.ts",
    "test:neurophoto:gq-all": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/neuro/gq-portrait/GQAllTester.ts",
    "test:neurophoto:hipster": "ts-node -r tsconfig-paths/register src/test-utils/tests/neuro/hipster-portrait/ModernHipsterTester.ts",
    "test:neurophoto:hipster-batch": "ts-node -r tsconfig-paths/register src/test-utils/tests/neuro/hipster-portrait/testModernHipsterPortraitBatch.ts",
    "test:neurophoto:all": "npm run test:neurophoto:direct && npm run test:neurophoto:real && npm run test:neurophoto:gq && npm run test:neurophoto:hipster",
    "test:neurophoto:generation": "ts-node -r tsconfig-paths/register src/test-utils/tests/neuro/direct/testNeuroPhotoGeneration.ts",
    "test:neurophoto:real-api": "cross-env NODE_ENV=test REAL_API_TEST=true npx tsx src/test-utils/tests/neuro/real-api/testNeuroPhotoRealAPI.ts",
    "test:api:detailed": "cross-env NODE_ENV=test npx tsx src/test-utils/scripts/test-api.ts --detailed --report",
    "test:api:endpoints": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/api/apiEndpointTest.ts",
    "test:api:endpoints:mock": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/test-utils/tests/api/apiEndpointTestMock.ts",
    "test:inngest:direct": "npx tsx src/test-utils/tests/runInngestTests.ts direct",
    "test:inngest:sdk": "npx tsx src/test-utils/tests/runInngestTests.ts sdk",
    "test:inngest:registration": "npx tsx src/test-utils/tests/runInngestTests.ts registration",
    "test:inngest:full": "npx tsx src/test-utils/tests/runInngestTests.ts full",
    "test:discover": "npx tsx src/test-utils/index.ts --discover",
    "test:watch": "jest --watch",
    "test:pulse": "cross-env NODE_ENV=test npx tsx src/test-utils/runPulseIntegrationTests.ts",
    "pulse-tests": "bash scripts/run-pulse-tests.sh",
    "test:image-to-video": "cross-env NODE_ENV=test npx tsx src/test-utils/runImageToVideoTests.ts",
    "image-to-video-tests": "bash scripts/run-image-to-video-tests.sh",
    "test:text-to-video": "cross-env NODE_ENV=test npx tsx src/test-utils/runTextToVideoTests.ts",
    "text-to-video-tests": "bash scripts/run-textToVideo-tests.sh",
    "api-tests": "bash scripts/run-api-tests.sh",
    "api-tests:detailed": "bash scripts/run-api-tests.sh --detailed --report",
    "api-tests:endpoints": "bash scripts/run-api-endpoints-tests.sh",
    "api-tests:endpoints:mock": "bash scripts/run-api-endpoints-mock-tests.sh",
    "inngest-tests": "bash scripts/run-inngest-tests.sh",
    "docker:test:inngest": "bash scripts/run-inngest-tests.sh",
    "docker:test:api": "bash scripts/run-api-tests-docker.sh",
    "docker:test:api:endpoints": "bash scripts/run-api-endpoints-tests-docker.sh",
    "check": "npm run lint && npm run build",
    "start:api": "nodemon --exec \"ts-node\" src/api.ts",
    "start:worker": "ts-node src/worker.ts",
    "test:memcheck": "node --expose-gc src/test-utils/memcheck.js",
    "test:inngest-prod": "ts-node scripts/test-inngest-production.ts",
    "send-message-to-owner": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/scripts/send-message-to-owner.ts",
    "test:chatbot": "ts-node src/test-utils/direct-chatbot-test.ts"
  },
  "dependencies": {
    "@allpepper/memory-bank-mcp": "^0.2.1",
