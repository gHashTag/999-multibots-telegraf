{
  "name": "neuro-blogger-telegram-bot",
  "version": "1.0.0",
  "description": "Neuro Blogger Telegram Bot",
  "author": "",
  "license": "ISC",
  "scripts": {
    "bots": "concurrently \"npm run dev:bots\" \"npm run dev:api\"",
    "services": "tsx src/api-server.ts",
    "check-port": "node scripts/check-webhook.js",
    "predev": "node scripts/kill-port.js 2999 3001 8288",
    "dev": "concurrently \"npm run bots\" \"npm run dev:inngest\"",
    "dev:services": "tsx src/api-server.ts",
    "dev:inngest": "npx inngest-cli@latest dev -u http://localhost:2999/api/inngest",
    "dev:web": "next dev -p 3000",
    "test": "jest",
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "build:tsc": "tsc && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts src",
    "lint:fix": "npm run lint -- --fix",
    "ngrok": "ngrok http 2999",
    "tunnel": "ssh -R 80:localhost:2999 localhost.run",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "dev:bots": "cd src && tsx watch bots.ts",
    "dev:api": "cd src && tsx watch api.ts",
    "prebuild": "rm -rf dist",
    "prepare:assets": "node scripts/prepare-assets.js",
    "start": "node dist/api.js",
    "typecheck": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@inngest/test": "^0.1.6",
    "@mtproto/core": "^6.3.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/multer": "^1.4.12",
    "@types/node-fetch": "2",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "elevenlabs": "^0.16.1",
    "envalid": "^7.3.1",
    "esbuild": "^0.25.2",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "form-data": "^4.0.1",
    "inngest": "^3.32.7",
    "jest-mock": "^29.7.0",
    "jszip": "^3.10.1",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "2",
    "openai": "^4.77.0",
    "pinata-web3": "^0.5.4",
    "replicate": "^0.32.0",
    "telegraf": "^4.16.3",
    "tslib": "^2.8.1",
    "undici": "^7.7.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@inngest/test": "^0.1.6",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/md5": "^2.3.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "node-config": "^0.0.2",
    "node-gyp": "^9.1.0",
    "nodemon": "^2.0.19",
    "pm2": "^5.2.0",
    "prettier": "^3.0.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
