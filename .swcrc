{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es2020",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false,
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@/utils/*": ["utils/*"],
      "@/config": ["config"],
      "@/controllers/*": ["controllers/*"],
      "@/dtos/*": ["dtos/*"],
      "@/exceptions/*": ["exceptions/*"],
      "@/interfaces/*": ["interfaces/*"],
      "@/middlewares/*": ["middlewares/*"],
      "@/models/*": ["models/*"],
      "@/routes/*": ["routes/*"],
      "@/services/*": ["services/*"],
      "@/core/*": ["core/*"],
      "@/axios/*": ["axios/*"],
      "@/price/*": ["price/*"],
      "@/store/*": ["store/*"],
      "@/handlers/*": ["handlers/*"],
      "@/helpers/*": ["helpers/*"],
      "@/commands/*": ["commands/*"],
      "@/inngest-functions/*": ["inngest-functions/*"],
      "@/types/*": ["types/*"],
      "@/test-utils/*": ["test-utils/*"],
      "@/menu/*": ["menu/*"],
      "@/scenes/*": ["scenes/*"]
    }
  },
  "module": {
    "type": "commonjs"
  }
}
